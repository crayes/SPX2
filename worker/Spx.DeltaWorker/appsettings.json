{
  "Delta": {
    "Enabled": false,
    "PollIntervalSeconds": 5
  },
  "SharePoint": {
    "SiteUrl": "https://rfaasp.sharepoint.com/sites/copilot",
    "DriveName": "Documentos",
    "FolderPath": "",
    "DeltaStateFile": ".state/sharepoint-delta.json",
    "OutputNdjsonPath": ".out/sharepoint-metadata.ndjson",
    "MaxItemsPerRun": 500,
    "IncludeFields": [
      "Modified By",
      "Tipo de Documento",
      "StatusProcessamento",
      "PalavraChave",
      "SubpastaOrigem",
      "DataProcessamento",
      "CategoriaInteligente",
      "ExtencaoArquivo",
      "Tamanhbytes",
      "CriadoPor",
      "DataModificacao",
      "DataCriacao"
    ],
    "HttpTimeoutSeconds": 100
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
