{
  "Delta": {
    "Enabled": false,
    "PollIntervalSeconds": 5
  },
  "SharePoint": {
    "SiteUrl": "https://rfaasp.sharepoint.com/sites/copilot",
    "DriveName": "Documentos",
    "FolderPath": "",
    "DeltaStateFile": ".state/sharepoint-delta.json",
    "OutputNdjsonPath": ".out/sharepoint-metadata.ndjson",
    "MaxItemsPerRun": 500,
    "ForceUpdate": false,
    "IncludeFields": [
      "Modified By",
      "Tipo de Documento",
      "StatusProcessamento",
      "PalavraChave",
      "SubpastaOrigem",
      "DataProcessamento",
      "CategoriaInteligente",
      "ExtencaoArquivo",
      "Tamanhbytes",
      "CriadoPor",
      "DataModificacao",
      "DataCriacao"
    ],
    "HttpTimeoutSeconds": 100
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/spx-delta-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 14,
          "shared": true
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
